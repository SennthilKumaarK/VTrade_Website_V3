
@model IEnumerable<DAL.ContactDetail>

<div class="card-body">
    <div class="table-responsive">
        <table class="table table-bordered" id="tblContactUsersInfoGrid" width="100%" cellspacing="0">
            <thead>
                <tr>
                    <th>
                        Date
                    </th>
                    <th>
                        Time
                    </th>
                    <th>
                        Name
                    </th>
                    <th>
                        Email
                    </th>
                    <th>
                        Mobile Number
                    </th>
                    <th>
                        Message
                    </th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    foreach (var mItem in Model)
                    {
                        <tr>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactDate)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactTime)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactName)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactEmail)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactNumber)
                            </td>
                            <td>
                                @Html.DisplayFor(modelItem => mItem.ContactMessage)
                            </td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $("#tblContactUsersInfoGrid").DataTable(
            {
                searching: false,
                ordering: false
            }
        );
    });
</script>

